<h1>This is Student Page it's for collecting your data</h1>

<h3>Enter Student Details</h3>
<form action="/student/create-student-data" method="post">
  <label for="name">Student Name :</label>
  <input type="text" name="name" required />
  <br />
  <label for="email">Email :</label>
  <input type="email" name="email" required />
  <br />
  <label for="contact">Contact no:</label>
  <input type="number" name="contact" maxlength="10" required />
  <br />
  <label for="batch">batch :</label>
  <input list="batch" name="batch" />
  <datalist id="batch">
    <option value="10thOct"></option>
    <option value="12thsept"></option>
    <option value="14thNov"></option>
  </datalist>
  <br />
  <label for="college">College :</label>
  <input type="text" name="college" required />
  <input type="submit" value="submit" />
</form>
